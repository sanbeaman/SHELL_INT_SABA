            </ui>
            <ui uid="1" type="bodyImageGlow" width="154" height="160" y='80' margin="110" fillColor="0x000000" fillAlpha="1" frameShape="roundRect" frameColor="0x000000" frameSize="2" frameAlpha="1" pulseColor="orange" time="1" ease="QuintEaseOut" syncType="add">
